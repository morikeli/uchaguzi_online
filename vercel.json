{
    "version": 2,
    "builds": [{
        "src": "src/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLambdaSize": "15mb", "runtime": "python3.9"}
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "static"
        }
      }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/wsgi.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        }
    ]
}
